<%= simple_form_for(@employee) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :id_number %>
      <%= f.input :address %>
      <%= f.input :date_of_birth %>
      <%= f.input :phone %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
