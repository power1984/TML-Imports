<%= simple_form_for(@bank) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
      <%= f.input :account_number %>
      <%= f.input :name %>
      <%= f.input :currency,as: :select,collection: ['USD','EURO','SRD'] %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
